{
    "name": "Screen Share Protection",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "storage",
        "notifications"
    ],
    "web_accessible_resources": ["./html/suggesttoenable.html"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_icon": "image.png",
        "default_popup": "config/popup.html"
    },
    "icons": {
        "512": "image.png"
    },
    "options_page": "config/main.html"

}